{
  "name": "plugin-ar",
  "description": "Arweave integration plugin for ElizaOS enabling permanent data storage and blockchain interactions",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "UNLICENSED",
  "author": "fenilmodi00",
  "keywords": [
    "plugin",
    "elizaos",
    "arweave",
    "blockchain",
    "permanent-storage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fenilmodi00/plugin-ar.git"
  },
  "homepage": "https://github.com/fenilmodi00/plugin-ar",
  "bugs": {
    "url": "https://github.com/fenilmodi00/plugin-ar/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    ".npmignore",
    "package.json",
    "tsup.config.ts"
  ],
  "dependencies": {
    "@elizaos/core": "latest",
    "arweave": "^1.15.7",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@elizaos/cli": "latest",
    "dotenv": "16.4.5",
    "prettier": "3.5.3",
    "tsup": "8.5.0",
    "typescript": "5.8.2"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "tsc --noEmit && tsup",
    "test": "bun test",
    "format": "prettier --write ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "zod": "^3.24.4"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "API_KEY": {
        "type": "string",
        "description": "API key for the service"
      }
    }
  },
  "gitHead": "d5bd5c43bfebeb7ac02f9e029f924cb6cd5c2ec7"
}
